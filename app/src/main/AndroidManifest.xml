<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="testnewapp.everythingforward.com.wellnessdoctorside">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayPatients"
            android:label="@string/title_activity_display_patients"
            android:theme="@style/AppTheme.NoActionBar" />

        <service android:name=".GetData" />

        <activity
            android:name=".PatientEmotions"
            android:label="@string/title_activity_patient_emotions"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>